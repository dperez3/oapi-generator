{
  "name": "oapi-generator",
  "version": "1.0.2-fix-dependencies.1",
  "description": "Generate OpenAPI documents from multiple Swagger v2 or Open API v3 documents.",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "main": "dist/index.js",
  "module": "dist/mylib.esm.js",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "cm": "git-cz",
    "release-dry": "standard-version --dry-run",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dperez3/oapi-generator.git"
  },
  "keywords": [
    "openapi",
    "swagger",
    "generator",
    "import",
    "proxy",
    "openapi3",
    "openapi2",
    "swagger2",
    "json"
  ],
  "author": "Dom Perez III <dperez3iii@gmail.com>",
  "license": "GPL-3.0",
  "peerDependencies": {
    "openapi-types": "1.3.4"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.2",
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "@types/console-ui": "^2.2.3",
    "@types/deep-extend": "^0.4.31",
    "@types/json-pointer": "^1.0.30",
    "@types/node": "^10.12.21",
    "@types/request-promise": "^4.1.42",
    "@types/yargs": "^12.0.8",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "standard-version": "^4.4.0"
  },
  "dependencies": {
    "console-ui": "^3.1.1",
    "deep-extend": "^0.6.0",
    "json-pointer": "^0.6.0",
    "openapi-types": "1.3.4",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "swagger2openapi": "5.1.0",
    "yargs": "12.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "bugs": {
    "url": "https://github.com/dperez3/oapi-generator/issues"
  },
  "homepage": "https://github.com/dperez3/oapi-generator#readme"
}
