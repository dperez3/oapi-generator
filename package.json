{
  "name": "oapi-generator",
  "version": "1.0.1",
  "description": "Generate OpenAPI documents from multiple Swagger v2 or Open API v3 documents.",
  "bin": "./run.js",
  "main": "index.js",
  "scripts": {
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "prettier": "prettier",
    "prettier-check": "prettier --check **/*.ts",
    "prettier-write": "prettier --write **/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cm": "git-cz",
    "release-dry": "standard-version --dry-run",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dperez3/oapi-generator.git"
  },
  "keywords": [
    "openapi",
    "swagger",
    "generator",
    "import",
    "proxy",
    "openapi3",
    "openapi2",
    "swagger2",
    "json"
  ],
  "author": "Dom Perez III <dperez3iii@gmail.com>",
  "license": "GPL-3.0",
  "peerDependencies": {
    "openapi-types": "1.3.4"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@types/deep-extend": "^0.4.31",
    "@types/json-pointer": "^1.0.30",
    "@types/request-promise": "^4.1.42",
    "@types/yargs": "^12.0.8",
    "typescript": "^3.3.1",
    "prettier": "1.16.4",
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "standard-version": "^4.4.0"
  },
  "dependencies": {
    "deep-extend": "0.6.0",
    "json-pointer": "0.6.0",
    "openapi-types": "1.3.4",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "swagger2openapi": "5.1.0",
    "yargs": "12.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/dperez3/oapi-generator/issues"
  },
  "homepage": "https://github.com/dperez3/oapi-generator#readme"
}
